@model IEnumerable<WebApplication2.Models.Empresa>

@{
    ViewBag.Title = "Index";
    Layout = "~/Views/Shared/_Layout_Empresa.cshtml";
}

<h2>Index</h2>

<p>
    @Html.ActionLink("Cadastre-se", "Create")
</p>
<table class="table">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.Cnpj)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Nome)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Login)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Senha)
        </th>
        <th></th>
    </tr>

    @foreach (var item in Model)
    {
<tr>
    <td>
        @Html.DisplayFor(modelItem => item.Cnpj)
    </td>
    <td>
        @Html.DisplayFor(modelItem => item.Nome)
    </td>
    <td>
        @Html.DisplayFor(modelItem => item.Login)
    </td>
    <td>
        @Html.DisplayFor(modelItem => item.Senha)
    </td>
    <td>
        @Html.ActionLink("Editar", "Edit", new { id = item.Id_Empresa }) |
        @Html.ActionLink("Detalhes", "Details", new { id = item.Id_Empresa }) |
        @Html.ActionLink("Excluir", "Delete", new { id = item.Id_Empresa })
    </td>
</tr>
}

</table>
